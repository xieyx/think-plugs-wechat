{
  "type": "think-admin-plugin",
  "name": "zoujingli/think-plugs-wechat",
  "license": "MIT",
  "description": "基于 ThinkAdmin 的 Wechat 模块",
  "authors": [
    {
      "name": "Anyon",
      "email": "zoujingli@qq.com"
    }
  ],
  "require": {
    "zoujingli/think-library": "^6.0|v6.0.x-dev",
    "zoujingli/wechat-developer": "^1.2|dev-master",
    "zoujingli/think-plugs-admin": "^1.0|dev-master",
    "zoujingli/think-plugs-static": "^1.0|dev-master",
    "zoujingli/think-plugs-installer": "^1.0|dev-master"
  },
  "autoload": {
    "psr-4": {
      "plugin\\wechat\\": "src"
    }
  },
  "extra": {
    "think": {
      "services": [
        "plugin\\wechat\\Service"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "zoujingli/think-plugs-installer": true
    }
  }
}
